.github/
.gitlab-ci.yml
.travis.yml
Jenkinsfile

# Docker
Dockerfile*
docker-compose*.yml
.docker/

# Local development
*.local
*.log
logs/
tmp/
temp/

# OS
.DS_Store
Thumbs.db

